{
    "manifest_version": 2,
    "name": "EmGAntiPub",
    "version": "1.1.1",
    "description": "Anti Pub for Mozilla.org",
    "icons": {
        "32": "icons/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.larep.fr/*"],
            "js": ["noPub.js"]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "@emg-antipub.emmguyot.com",
            "data_collection_permissions": {
                "required": ["none"]
            }
        }
    }
}